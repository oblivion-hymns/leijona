{
	"format": {
		"type": "html",
		"separateNumbers": true,
		"numberSeparator": ","
	},
	"count": {
		"comments": true,
		"trivial": true,
		"empty": true
	},
	"commentCharacters": [
		"/*",
		"*/",
		"* ",
		"//",
		"#",
		"<!--"
	],
	"exclude": {
		"paths": [
			".git/",
			"node_modules/",
			"vendor/",
			"package-lock.json",
			"package.json"
		],
		"fileTypes": [
			"gif",
			"jpeg",
			".jpg",
			"png",
			"svg",
			"tiff"
		]
	}
}
